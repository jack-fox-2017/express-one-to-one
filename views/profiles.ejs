<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title></title>
</head>
<body>
  <h1>Profiles</h1>
  <table border="1">
    <tr>
      <th>ID</th>
      <th>Home town</th>
      <th>Birth year</th>
      <th>relationship status</th>
      <th>user id</th>
    </tr>
    <% data.forEach (d =>{%>
      <tr>
        <td>
          <%= d.id %>
        </td>
        <td>
          <%= d.hometown %>
        </td>
        <td>
          <%= d.birth_year %>
        </td>
        <td>
          <%= d.relationship_status %>
        </td>
        <td>
          <%= d.user_id %>
        </td>
        <td><a href="/profiles/edit/<%= d.id %>"><button type="button" name="button">Edit</button></a></td>
        <td><a href="/profiles/delete/<%= d.id %>"><button type="button" name="button">Delete</button></a></td>
      </tr>
      <% }) %>
  </table>
  <br>
  <left>
    <form action="/profiles" method="post">
      <label><b>Home Town<b></label>
      <input type="text" name="hometown" placeholder="please input your Home Town">
      <br>
      <label><b>Birth Year<b></label>
      <input type="text" name="birth_year" placeholder="please input your birth year">
      <br>
      <label><b>relationship status<b></label>
      <input type="text" name="relationship_status" placeholder="please input your relationship_status">
      <br>
    </form><br>
    <select name="user_id">
      <%data1.forEach(d=>{%>
      <option value ="<%=d.id%>"><%= d.firstname %></option>
      <% }) %>
    </select>
      <button type="submit" name="submit">Submit </button>
  </left>
  <br>
  <a href="/">back to home</a>
</body>
</html>
