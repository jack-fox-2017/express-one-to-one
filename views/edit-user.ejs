<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Edit contact</title>
  </head>
  <body>
    <h1>Edit Users</h1>
    <% let d = data %>
    <form action="/users/edit/<%= d.id %>" method="post">
      <input type="text" name="username" placeholder="Username" value="<%= d.username %>"><br>
      <input type="text" name="firstname" placeholder="Firstname" value="<%= d.firstname %>"><br>
      <input type="text" name="lastname" placeholder="Lastname" value="<%= d.lastname %>"><br>
      <input type="text" name="email" placeholder="Email" value="<%= d.email %>"><br><br>
      <input type="submit" value="Submit">
    </form>
  </body>
</html>
