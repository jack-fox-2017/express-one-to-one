<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Edit Contact</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bulma/0.5.0/css/bulma.min.css">
  </head>
  <body>
    <center>
      <section class="section">
      <div class="container">
    <h1 class="title">Edit User</h1>
    <h2>
      <%dataU.forEach(function(d){ %>
      <form action="/users/edit/<%=d.id%>" method="post">
        Username:<br>
        <input type="text" name="username" value="<%=d.username%>"><br>
        Firstname:<br>
        <input type="text" name="firstname" value="<%=d.firstname%>"> <br>
        Lastname:<br>
        <input type="text" name="lastname" value="<%=d.lastname%>"><br>
        Email:<br>
        <input type="text" name="email" value="<%=d.email%>"><br>
        <br>
        <button type="submit" name="button">Save Changes</button> <br>
      </form>
    </h2>
    <%})%>
  </div>
</section>
  </center>
  </body>
</html>
