<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Edit Profile</title>
  </head>
  <body>
    <h1>Edit Profile</h1>
   <br>
   <br>
   <form action="/profile/edit/<%= data[0].id %>" method="post">
     <fieldset>
       <input type="text" name="hometown" value="<%= data[0].hometown %>"><br>
       <input type="text" name="birth_year" value="<%= data[0].birth_year %>"><br>
       <input type="text" name="relationship_status" value="<%= data[0].relationship_status %>"><br>

       <select name="user_id">
       <% dataUsers.forEach(z=> { %>
         <option value="<%= z.id %>"><%= z.firstname %></option>
         <% })%>
       </select>

      <button type="submit">save</button>
     </fieldset>
   </form>
   <br><br>
   <a href="/">Home</a>
 </body>
</html>
